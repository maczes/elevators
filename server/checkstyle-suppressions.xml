<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress files=".+\.(?:properties|txt|xml|csv|sh|thrift|html|sql|eot|ttf|woff|css|png)$" checks=".*"/>
    <suppress checks="HideUtilityClassConstructor" files="ElevatorApplication.java"/>

    <!-- JavadocStyle/JavadocPackage enforces existence of package-info.java package-level Javadoc -->
    <suppress files=".+\.(?:java)$" checks="JavadocStyle" />
    <suppress files=".+\.(?:java)$" checks="JavadocPackage" />
    <suppress files=".+\.(?:java)$" checks="JavadocVariable" />

    <!-- Generated code should not be subjected to checkstyle. -->
    <suppress files="[/\\].*[/\\]generated.*[/\\]" checks="." />
    
    <!-- Suppress test resources -->
    <suppress files="[/\\]src[/\\]test[/\\]resources.*" checks="." />
</suppressions>